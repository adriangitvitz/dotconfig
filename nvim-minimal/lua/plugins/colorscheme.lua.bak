return {
  "iceberg-tokyo.nvim",
  dev = true,
  priority = 1000,
  config = function()
    -- Load the keratoconus-optimized warm amber theme
    require('iceberg-tokyo').init()

    -- Additional keratoconus-specific highlight overrides
    vim.api.nvim_create_autocmd("ColorScheme", {
      callback = function()
        -- Keep warm color overrides for maximum clarity
        -- Search: Warm bronze background with peachy text
        vim.api.nvim_set_hl(0, "Search", { bg = "#5A4A32", fg = "#E8D4B8", bold = true })
        vim.api.nvim_set_hl(0, "IncSearch", { bg = "#D4A574", fg = "#2B1F1A", bold = true })

        -- Visual selection: Warm bronze
        vim.api.nvim_set_hl(0, "Visual", { bg = "#5A4A32", fg = "NONE" })

        -- Matching parentheses: Use warm cream with bold/underline for visibility
        vim.api.nvim_set_hl(0, "MatchParen", { fg = "#E8DCC8", bold = true, underline = true })
      end,
    })
  end,
}
